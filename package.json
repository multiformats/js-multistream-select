    "it-handshake": "^2.0.0",
    "it-length-prefixed": "^5.0.0",
    "it-pipe": "^1.0.1",
    "it-reader": "^3.0.0",
    "p-defer": "^3.0.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@types/bl": "^5.0.1",
    "@types/debug": "^4.1.7",
    "aegir": "^36.1.1",
    "it-pair": "^1.0.0",
    "p-timeout": "^4.0.0",
    "streaming-iterables": "^5.0.2",
    "timeout-abort-controller": "^2.0.0",
    "util": "^0.12.4",
    "varint": "^6.0.0"
  },
